{
	"name": "Tunic",
	"version": "1.0",
	"author": "Gabriel Wicke, Timwi and HoodWiki",
	"url": "https://www.mediawiki.org/wiki/Skin:Tunic",
	"descriptionmsg": "tunic-desc",
	"namemsg": "skinname-tunic",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"tunic": {
			"class": "SkinMustache",
			"args": [ {
				"name": "tunic",
				"responsive": true,
				"templateDirectory": "skins/Tunic/templates",
				"styles": [
					"skins.tunic"
				],
				"messages": [
					"sitetitle",
					"search",
					"tagline",
					"navigation-heading"
				],
				"scripts": [
					"skins.tunic.js"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"Tunic": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.tunic": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"normalize": true,
				"elements": true,
				"content": true,
				"content-parser-output": true,
				"interface": true,
				"logo": true
			},
			"styles": {
				"resources/screen-common.less": {
					"media": "screen"
				},
				"resources/screen-desktop.less": {
					"media": "screen and (min-width: 751px)"
				},
				"resources/screen-mobile.less": {
					"media": "screen and (max-width: 750px)"
				},
				"resources/print.css": {
					"media": "print"
				}
			}
		},
		"skins.tunic.js": {
			"scripts": [
				"resources/main.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Tunic"
	},
	"ResourceModuleSkinStyles": {
		"tunic": {
			"+ext.echo.styles.badge": "resources/extensions/echo.styles.badge.less"
		}
	},
	"manifest_version": 1
}
